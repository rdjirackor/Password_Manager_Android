<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".EnterDetails">


    <EditText
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:hint="Password"
        android:textColorHint="@color/black"
        android:background="@drawable/back"
        android:ems="10"
        android:id="@+id/editTextTextPersonName"
        app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.423" app:layout_constraintVertical_bias="0.474"
        android:minHeight="48dp"/>
    <EditText
        android:layout_width="300dp"
        android:layout_height="153dp"
        android:inputType="textPersonName"
        android:hint="Notes (Optional)"
        android:textColorHint="@color/black"
        android:background="@drawable/back"
        android:ems="10"
        android:id="@+id/editTextTextPersonName2"
        app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.423" app:layout_constraintVertical_bias="0.702"/>
    <EditText
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:hint="Username"
        android:ems="10"
        android:textColorHint="@color/black"
        android:background="@drawable/back"
        android:id="@+id/editTextTextPersonName3"
        app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.423" app:layout_constraintVertical_bias="0.335"
        android:minHeight="48dp"/>
    <EditText
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:hint="Account For"
        android:background="@drawable/back"
        android:ems="10"
        android:textColorHint="@color/black"
        app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
        android:id="@+id/editTextTextPersonName4"
        app:layout_constraintHorizontal_bias="0.423" app:layout_constraintVertical_bias="0.187"
        android:minHeight="48dp"/>
    <EditText
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:background="@drawable/back"
        android:textColorHint="@color/black"
        app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
        android:hint="Title"
        android:ems="10"
        android:id="@+id/editTextTextPersonName5"
        app:layout_constraintHorizontal_bias="0.423" app:layout_constraintVertical_bias="0.048"
        android:minHeight="48dp"/>
    <Button
        android:text="Save"
        android:layout_width="103dp"
        android:background="@drawable/background"
        android:layout_height="56dp" android:id="@+id/button"
        app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2"
        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.798"
        app:layout_constraintVertical_bias="0.516"/>
</androidx.constraintlayout.widget.ConstraintLayout>